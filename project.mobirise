range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <!---->\n    <!--Overlay-->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <!--Container-->\n    <div class=\"container\">\n        <div class=\"col-md-12\">\n            <div class=\"media-container-row\">\n                <div class=\"mbr-figure\" mbr-style=\"{'width': mediaSize + '%'}\">\n                    <a href=\"rosa-albina-reposteria-en-cabimas.html\"><img src=\"@PROJECT_PATH@/assets/images/rosaalbina2-320x300.jpg\" alt=\"Rosa Albina Reposteria \" title=\"Rosa Albina Reposteria \"></a>\n                </div>\n                <div class=\"align-left aside-content\">\n                    <h2 class=\"mbr-title pt-2 mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">\n                        \n                    <div><span style=\"color: inherit; font-size: 3rem;\">Rosa Albina Repostería</span><br></div></h2>\n                    <div class=\"mbr-section-text\">\n                        <p class=\"mbr-text text1 pt-2 mbr-light mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".text1\"><b>Información:</b>\n<br>\n<br><b>Direccion:</b> Calle Chile, con Cjon el Pirata, Delicias Nuevas. Cabimas, Zulia.<br><b>Teléfono:</b> 0412-6578667<br><b>Correo:</b>&nbsp;N/A<br><b>Instagram:</b>&nbsp;<a href=\"https://www.instagram.com/rosalbina_reposteria/\" class=\"text-warning\">@rosalbina_reposteria</a><br><b>Horario:</b> Lunes a Sábado de 9:00am a 5:00pm\n<br><b>Métodos de pago:&nbsp;</b>Pago Movil, Divisas, Procesadores de Pago</p>\n                        <p class=\"mbr-text text2 pt-4 mbr-light mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showSubtitle2\" data-app-selector=\".text2, .mbr-section-btn\">\n   